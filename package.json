{
  "name": "@dicoauto/toolbox",
  "repository": "https://github.com/dicoauto/toolbox",
  "version": "0.0.1",
  "description": "General component library for building react apps for diconium auto",
  "scripts": {
    "build:clean": "rm -rf dist coverage releases storybook-static",
    "build:clean:all": "npm run build:clean && rm -rf node_modules",
    "build:css:watch": "npx tailwindcss -i ./styles/toolbox.css -o ./dist/toolbox.css --watch",
    "build:css": "npx tailwindcss -i ./styles/toolbox.css -o ./dist/toolbox.css",
    "build:prod": "npm run build:css && NODE_ENV=production rollup -c",
    "build:release": "npm run build:clean && mkdir releases && npm run build:prod && npm pack --pack-destination ./releases",
    "test": "jest",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run build:prod && build-storybook",
    "lint": "eslint ./src --ext js,ts,tsx,jsx",
    "lint:fix": "npm run lint -- --fix",
    "generate:component": "hygen component new --name $1"
  },
  "author": "Diconium Auto GmbH - Team Web",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^7.0.10",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.35",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "chromatic": "^6.17.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hygen": "^6.2.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.7",
    "pretty": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.20.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook-addon-pseudo-states": "^1.15.5",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.13.0"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "react-swipeable": "^7.0.0"
  }
}